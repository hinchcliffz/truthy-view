{
  "name": "@hinchcliffz/truthy-view",
  "version": "0.0.1",
  "description": "View a little deeper",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:file": "babel src --no-comments --out-file ./dist/standalone/truthyview.js",
    "start": "npm run build && nodemon dist/index.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "jest test --coverage",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "checkAll": "npm run format:check && npm run lint:check && npm run test",
    "coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "keywords": [
    "Truth"
  ],
  "author": "Zachary Hinchcliff <hinchcliffz@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/runtime-corejs3": "^7.17.2",
    "@babel/standalone": "^7.17.6",
    "axios": "^0.26.0",
    "babel-plugin-transform-modules-requirejs-babel": "^0.1.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "core-js": "3",
    "ipaddr.js": "^2.0.1",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.1.0",
    "js-cookie": "^3.0.1",
    "macaddress": "^0.5.2",
    "puppeteer": "^13.4.0",
    "requirejs": "^2.3.6",
    "requirejs-babel7": "^1.3.2",
    "systemjs": "^6.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
